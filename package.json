{
    "name": "kharbga.net",
    "version": "1.0.0",
    "description": "An online board strategy game based on the ancient games of Kharbga and Seega from North Africa. \r The rules of this online game are close to the rules of Seega as documented by various online resources.\r See references below. However, the exact rules enforced by this program are based on the version played in the south of Tunisia.",
    "main": "systemjs-angular-loader.js",
    "private": true,
    "dependencies": {
        "@popperjs/core": "^2.4.0",
        "angular": "^1.8.3",
        "angular-ui-bootstrap": "^2.5.0",
        "angular-ui-router": "^1.0.26",
        "bootstrap": "^4.5.0",
        "bootstrap-combobox": "^1.0.2",
        "jqueryrouter": "^1.2.0",
        "popper.js": "^1.16.1",
        "vue": "^2.6.11"
    },
    "devDependencies": {
        "babel-core": "^6.0.0",
        "babel-loader": "^7.0.0",
        "babel-preset-env": "^1.6.1",
        "cross-env": "^4.0.0",
        "css-loader": "^3.5.3",
        "expose-loader": "^0.7.5",
        "file-loader": "^0.11.1",
        "surge": "^0.21.3",
        "vue-loader": "^14.2.2",
        "vue-template-compiler": "^2.3.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0"
    },
    "scripts": {
        "local": "node app.js",
        "dev": "webpack-dev-server --inline --hot",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "predeploy": "npm run build",
        "deploy": "surge --project . --domain kharbga.surge.sh",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
